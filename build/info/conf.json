{
  "opts": {
    "recurse": true,
    "template": "build/info"
  },
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "includePattern": "\\.js$",
    "exclude": ["dist", "node_modules/ol/node_modules"]
  },
  "plugins": [
    "jsdoc-plugin-intersection",
    "jsdoc-plugin-typescript",
    "build/info/api-plugin",
    "build/info/define-plugin",
    "build/info/virtual-plugin"
  ],
  "typescript": {
    "moduleRoot": "node_modules/ol"
  }
}
