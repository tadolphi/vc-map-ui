      "legend": []
    },
    {
      "type": "DeclarativeStyleItem",
      "name": "Tree-ID",
      "declarativeStyle": {
        "show": "true",
        "color": {
          "conditions": [
            [
              "${TREE_ID} > 0",
              "mix(color('#00FF00') * (${TREE_ID} % 21) / 20, ${COLOR}, 0.5)"
            ],
            [
              "true",
              "${COLOR}"
            ]
          ]
        },
        "pointSize": 2
      },
      "legend": []
    },
    {
      "type": "DeclarativeStyleItem",
      "name": "Distance",
      "declarativeStyle": {
        "show": "true",
        "color": {
          "conditions": [
            [
              "${DISTANCE} > 4",
              "mix(color('#ffff00'), color('#bd0000'), clamp(0, 1, (${DISTANCE}-4)/5))"
            ],
            [
              "true",
              "${COLOR}"
            ]
          ]
        },
        "pointSize": 2
      },
      "legend": []
    },
