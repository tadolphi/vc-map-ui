{
  "name": "virtualcitymap-ui",
  "version": "5.0.0",
  "private": true,
  "scripts": {
    "start": "vite",
    "test": "node --experimental-json-modules node_modules/mocha/bin/_mocha --exit --require tests/setupJsdom.js --require tests/setup.js --recursive \"tests/**/*spec.js\"",
    "preview": "vite preview",
    "build": "node build/build.mjs",
    "lint": "eslint \"{src,tests,build}/**/*.{js,mjs,vue}\""
  },
  "dependencies": {
    "@mdi/font": "^5.9.55",
    "@vcmap/cesium": "^1.87.2",
    "@vcmap/core": "^5.0.0-rc.3",
    "@vcsuite/check": "^1.0.3",
    "@vcsuite/logger": "^1.0.1",
    "@vcsuite/parsers": "^1.0.1",
    "@vcsuite/ui-components": "^0.1.5",
    "@vue/composition-api": "^1.4.5",
    "fuse.js": "^6.4.6",
    "ol": "^6.9.0",
    "rxjs": "^6.6.6",
    "uuid": "^8.3.2",
    "vue": "^2.6.11",
    "vue-i18n": "^8.24.1",
    "vuetify": "^2.5.3"
  },
  "devDependencies": {
    "@vcmap/rollup-plugin-vcs-ol": "^1.0.1",
    "@vcsuite/eslint-config": "^2.0.3",
    "@vue/test-utils": "^1.3.0",
    "c8": "^7.10.0",
    "canvas": "^2.8.0",
    "chai": "^4.3.4",
    "eslint": "^8.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "jsdoc": "^3.6.7",
    "jsdoc-plugin-typescript": "^2.0.6",
    "jsdom": "^19.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^9.1.3",
    "sass": "1.32.13",
    "sinon": "^12.0.1",
    "sinon-chai": "^3.7.0",
    "typescript": "^4.2.4",
    "unplugin-vue-components": "^0.17.15",
    "vite": "^2.7.13",
    "vite-plugin-vue2": "^1.9.3",
    "vue-loader": "^16.6.0",
    "vue-template-compiler": "^2.6.14",
    "walk": "^2.3.15"
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "selector-pseudo-element-no-unknown": [
        true,
        {
          "ignorePseudoElements": [
            "v-deep"
          ]
        }
      ]
    }
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "@vcsuite/eslint-config/vue"
    ],
    "plugins": [
      "jsdoc"
    ],
    "settings": {
      "import/resolver": {
        "alias": {
          "map": [
            [
              "@",
              "./src"
            ]
          ]
        }
      }
    },
    "rules": {
      "jsdoc/check-tag-names": "off"
    }
  },
  "browserslist": [
    "last 2 Chrome versions",
    "not Chrome < 97",
    "last 2 Safari versions",
    "not Safari < 15",
    "last 2 iOS versions",
    "not iOS < 15",
    "last 2 Firefox versions",
    "not Firefox < 91",
    "last 2 Edge versions",
    "not Edge < 97"
  ],
  "type": "module"
}
