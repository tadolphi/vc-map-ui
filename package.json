{
  "name": "@vcmap/ui",
  "version": "5.0.0-rc.6",
  "author": "Virtual City Systems",
  "license": "MIT",
  "scripts": {
    "start": "vite",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "coverage:gitlab": "vitest run --coverage --reporter junit --outputFile test-report.xml",
    "build-preview": "node build/buildPreview.js",
    "preview": "vite preview",
    "build": "node build/build.js",
    "lint": "eslint \"{src,tests,build}/**/*.{js,mjs,vue}\"",
    "prepack": "node build/build.js",
    "install-plugins": "cd plugins && npm i --omit peer --no-package-lock --force"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/virtualcitySYSTEMS/map-ui.git"
  },
  "files": [
    "index.js",
    "start.js",
    "map.config.json",
    "dist/",
    "src/",
    "plugins/",
    "lib/",
    "build/",
    "config/",
    "!plugins/node_modules/"
  ],
  "dependencies": {
    "@mdi/font": "^5.9.55",
    "@vcsuite/check": "^1.1.2",
    "@vcsuite/logger": "^1.0.1",
    "@vcsuite/parsers": "^1.0.1",
    "rxjs": "^6.6.6",
    "uuid": "^8.3.2",
    "vue-i18n": "^8.24.1"
  },
  "peerDependencies": {
    "@vcmap/cesium": "~1.92.1",
    "@vcmap/core": "^5.0.0-rc.10",
    "@vcsuite/ui-components": "~0.1.13",
    "@vue/composition-api": "~1.4.5",
    "ol": "~6.13.0",
    "vue": "~2.6.14",
    "vuetify": "~2.6.3"
  },
  "devDependencies": {
    "@vcmap/rollup-plugin-vcs-ol": "^1.0.1",
    "@vcsuite/eslint-config": "^2.1.1",
    "@vue/test-utils": "^1.3.0",
    "c8": "^7.11.0",
    "eslint": "^8.9.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "jest-canvas-mock": "^2.3.1",
    "jsdoc": "^3.6.7",
    "jsdoc-plugin-typescript": "^2.0.6",
    "jsdom": "^19.0.0",
    "rollup-plugin-strip-pragma": "^1.0.0",
    "sass": "1.32.13",
    "typescript": "^4.2.4",
    "unplugin-vue-components": "^0.17.15",
    "vite": "^2.8.6",
    "vite-plugin-vue2": "^1.9.3",
    "vitest": "^0.6.1",
    "vue-loader": "^16.6.0",
    "vue-template-compiler": "^2.6.14"
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "selector-pseudo-element-no-unknown": [
        true,
        {
          "ignorePseudoElements": [
            "v-deep"
          ]
        }
      ]
    }
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "@vcsuite/eslint-config/vue"
    ],
    "plugins": [
      "jsdoc"
    ],
    "settings": {
      "import/resolver": {
        "alias": {
          "map": [
            [
              "@vcmap/ui",
              "./index.js"
            ]
          ]
        }
      }
    },
    "rules": {
      "jsdoc/check-tag-names": "off"
    }
  },
  "browserslist": [
    "last 2 Chrome versions",
    "not Chrome < 97",
    "last 2 Safari versions",
    "not Safari < 15",
    "last 2 iOS versions",
    "not iOS < 15",
    "last 2 Firefox versions",
    "not Firefox < 91",
    "last 2 Edge versions",
    "not Edge < 97"
  ],
  "engines": {
    "node": "^16.14.0",
    "npm": "^8.3.1"
  },
  "type": "module"
}
