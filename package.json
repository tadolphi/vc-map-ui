{
  "name": "virtualcitymap-ui",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:ci": "vue-cli-service test:unit tests/*",
    "test:local": "vue-cli-service test:unit tests/* --watch",
    "coverage": "nyc npm run test:ci",
    "lint": "vue-cli-service lint",
    "lint:fix": "vue-cli-service lint --fix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build:docs": "./node_modules/.bin/jsdoc --verbose -c jsdoc-conf.js -r ./src -d docs",
    "lint:css": "stylelint '**/*.vue' --syntax scss",
    "lint-fix:css": "stylelint '**/*.vue' --fix"
  },
  "dependencies": {
    "@mdi/font": "^5.9.55",
    "@vcmap/core": "git://github.com/virtualcitySYSTEMS/map-core.git#feature-npmPackage",
    "core-js": "^3.6.5",
    "fuse.js": "^6.4.6",
    "rxjs": "^6.6.6",
    "sortablejs": "^1.13.0",
    "storybook": "6.2.9",
    "uuid": "^8.3.2",
    "vue": "^2.6.11",
    "vue-click-outside": "^1.1.0",
    "vue-i18n": "^8.24.1",
    "vuedraggable": "^2.24.3",
    "vuetify": "^2.4.6",
    "vuetify-draggable-treeview": "^0.0.6",
    "vuex": "^3.4.0",
    "vuex-map-fields": "^1.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@intlify/eslint-plugin-vue-i18n": "^0.11.1",
    "@storybook/addon-actions": "6.2.9",
    "@storybook/addon-docs": "6.2.9",
    "@storybook/addon-essentials": "6.2.9",
    "@storybook/addon-links": "6.2.9",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/vue": "6.2.9",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^5.2.4",
    "@vcsuite/eslint-config": "^1.0.0",
    "@vue/cli-plugin-babel": "^4.5.12",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-unit-mocha": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/test-utils": "^1.0.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.1.2",
    "chai-spies": "^1.0.0",
    "css-loader": "^5.2.4",
    "eslint": "^7.26.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsdoc": "^32.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jsdoc": "^3.6.6",
    "jsdoc-vuejs": "^3.0.9",
    "mocha": "^8.3.2",
    "mocha-junit-reporter": "^2.0.0",
    "nyc": "^15.1.0",
    "sass": "^1.32.12",
    "sass-loader": "^10.1.1",
    "sinon": "^10.0.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-processor-html": "^1.0.0",
    "vue-cli-plugin-vuetify": "~2.2.2",
    "vue-cli-plugin-webpack-bundle-analyzer": "~4.0.0",
    "vue-svg-loader": "^0.16.0",
    "vue-template-compiler": "^2.6.12",
    "vuetify-loader": "^1.7.0"
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "selector-pseudo-element-no-unknown": [
        true,
        {
          "ignorePseudoElements": [
            "v-deep"
          ]
        }
      ]
    }
  }
}
