{
  "name": "virtualcitymap-ui",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:ci": "vue-cli-service test:unit tests/*",
    "test:local": "vue-cli-service test:unit tests/* --watch",
    "coverage": "nyc npm run test:ci",
    "lint": "vue-cli-service lint",
    "lint:fix": "vue-cli-service lint --fix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build:docs": "./node_modules/.bin/jsdoc --verbose -c jsdoc-conf.js -r ./src -d docs",
    "lint:css": "stylelint '**/*.vue' --syntax scss",
    "lint-fix:css": "stylelint '**/*.vue' --fix"
  },
  "dependencies": {
    "@mdi/font": "^5.9.55",
    "@vcmap/cesium": "^1.79.1",
    "@vcmap/core": "git://github.com/virtualcitySYSTEMS/map-core.git#master",
    "@vcsuite/check": "^1.0.2",
    "@vcsuite/logger": "^1.0.0",
    "@vcsuite/parsers": "^1.0.0",
    "@vue/composition-api": "^1.0.0-rc.11",
    "fuse.js": "^6.4.6",
    "ol": "^6.5.0",
    "rxjs": "^6.6.6",
    "sortablejs": "^1.13.0",
    "uuid": "^8.3.2",
    "vue": "^2.6.11",
    "vue-click-outside": "^1.1.0",
    "vue-i18n": "^8.24.1",
    "vuedraggable": "^2.24.3",
    "vuetify": "^2.5.3",
    "vuetify-draggable-treeview": "^0.0.6"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "sass": "1.32.13",
    "typescript": "^4.2.4",
    "vite": "^2.4.1",
    "vite-plugin-components": "^0.12.1",
    "vite-plugin-vue2": "^1.7.2",
    "vue-template-compiler": "^2.6.14"
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "selector-pseudo-element-no-unknown": [
        true,
        {
          "ignorePseudoElements": [
            "v-deep"
          ]
        }
      ]
    }
  },
  "browserslist": [
    "last 2 Chrome versions",
    "not Chrome < 86",
    "last 2 Safari versions",
    "not Safari < 14",
    "last 2 iOS versions",
    "not iOS < 12.5",
    "last 2 Firefox versions",
    "not Firefox < 78",
    "last 2 Edge versions",
    "not Edge < 86"
  ]
}
