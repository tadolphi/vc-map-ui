{
  "name": "@vcmap/ui",
  "version": "5.0.0-rc.20",
  "author": "Virtual City Systems",
  "license": "MIT",
  "scripts": {
    "start": "vite",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "coverage:gitlab": "vitest run --coverage --reporter junit --outputFile test-report.xml",
    "build-preview": "node build/buildPreview.js",
    "preview": "vite preview",
    "build": "node build/build.js",
    "lint": "eslint \"{src,tests,build}/**/*.{js,mjs,vue}\" index.js",
    "prepack": "node build/build.js",
    "install-plugins": "cd plugins && npm i --omit peer --no-package-lock --force"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/virtualcitySYSTEMS/map-ui.git"
  },
  "files": [
    "index.js",
    "start.js",
    "index.html",
    "map.config.json",
    "dist/",
    "src/",
    "plugins/",
    "lib/",
    "build/",
    "config/",
    "!plugins/node_modules/"
  ],
  "dependencies": {
    "@mdi/font": "^5.9.55",
    "@vcsuite/check": "^1.1.2",
    "@vcsuite/logger": "^1.0.1",
    "@vcsuite/parsers": "^1.0.1",
    "uuid": "^8.3.2",
    "vue-i18n": "^8.24.1"
  },
  "peerDependencies": {
    "@vcmap-cesium/engine": "~1.1.2",
    "@vcmap/core": "~5.0.0-rc.26",
    "ol": "~7.1.0",
    "vue": "~2.7.3",
    "vuetify": "~2.6.7"
  },
  "devDependencies": {
    "@vcmap/rollup-plugin-vcs-ol": "^1.0.2",
    "@vcsuite/eslint-config": "^2.1.1",
    "@vitest/coverage-c8": "^0.24.3",
    "@vue/test-utils": "^1.3.3",
    "eslint": "^8.9.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "jest-canvas-mock": "^2.4.0",
    "jsdoc": "^3.6.7",
    "jsdoc-plugin-intersection": "^1.0.4",
    "jsdoc-plugin-typescript": "^2.0.6",
    "jsdom": "^19.0.0",
    "rollup-plugin-strip-pragma": "^1.0.0",
    "sass": "1.32.13",
    "typescript": "^4.2.4",
    "vite": "^3.2.0",
    "vite-plugin-vue2": "^2.0.2",
    "vitest": "^0.24.3"
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "selector-pseudo-element-no-unknown": [
        true,
        {
          "ignorePseudoElements": [
            "v-deep"
          ]
        }
      ]
    }
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "@vcsuite/eslint-config/vue"
    ],
    "plugins": [
      "jsdoc"
    ],
    "settings": {
      "import/resolver": {
        "alias": {
          "map": [
            [
              "@vcmap/ui",
              "./index.js"
            ]
          ]
        }
      }
    },
    "rules": {
      "jsdoc/check-tag-names": "off"
    }
  },
  "browserslist": [
    "last 2 Chrome versions",
    "not Chrome < 97",
    "last 2 Safari versions",
    "not Safari < 15",
    "last 2 iOS versions",
    "not iOS < 15",
    "last 2 Firefox versions",
    "not Firefox < 91",
    "last 2 Edge versions",
    "not Edge < 97"
  ],
  "engines": {
    "node": "^16.14.0",
    "npm": "^8.3.1"
  },
  "type": "module",
  "exports": {
    ".": "./index.js",
    "./config/*.json": "./config/*.json",
    "./src/assets/logo.svg": "./src/assets/logo.svg",
    "./src/assets/logo-mobile.svg": "./src/assets/logo-mobile.svg",
    "./build/commonViteConfig.js": "./build/commonViteConfig.js",
    "./build/buildHelpers.js": "./build/buildHelpers.js",
    "./build/getPluginProxies.js": "./build/getPluginProxies.js",
    "./build/determineHost.js": "./build/determineHost.js",
    "./src/styles/variables.scss": "./src/styles/variables.scss"
  }
}
