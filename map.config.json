    {
      "type": "vcs.vcm.util.style.DeclarativeStyleItem",
      "name": "Tree-ID",
      "title": {
        "en": "Tree-ID",
        "de": "Tree-ID"
      },
      "declarativeStyle": {
        "show": "true",
        "color": {
          "conditions": [
            [
              "${TREE_ID} > 0",
              "mix(color('#00FF00') * (${TREE_ID} % 21) / 20, ${COLOR}, 0.5)"
            ],
            [
              "true",
              "${COLOR}"
            ]
          ]
        },
        "pointSize": 2
      },
      "legend": []
    },
    {
      "type": "vcs.vcm.util.style.DeclarativeStyleItem",
      "name": "Distance",
      "title": {
        "en": "Distance",
        "de": "Distance"
      },
      "declarativeStyle": {
        "show": "true",
        "color": {
          "conditions": [
            [
              "${DISTANCE} > 4",
              "mix(color('#ffff00'), color('#bd0000'), clamp(0, 1, (${DISTANCE}-4)/5))"
            ],
            [
              "true",
              "${COLOR}"
            ]
          ]
        },
        "pointSize": 2
      },
      "legend": []
    },
    {
      "type": "vcs.vcm.util.style.DeclarativeStyleItem",
      "name": "Classification",
      "title": {
        "en": "Classification",
        "de": "Classification"
      },
      "declarativeStyle": {
        "show": "true",
        "color": {
          "conditions": [
            [
              "${SEMANTIC} === 1",
              "mix(color('#9f9f61'), ${COLOR}, 0.3)"
            ],
            [
              "${SEMANTIC} === 3",
              "mix(color('#dc0000'), ${COLOR}, 0.3)"
            ],
            [
              "${SEMANTIC} === 4",
              "mix(color('#06ad16'), ${COLOR}, 0.3)"
            ],
            [
              "true",
              "${COLOR}"
            ]
          ]
        },
        "pointSize": 2
      },
      "toggleOptions": [
        {
          "title": "Buildings",
          "condition": "${SEMANTIC} === 1",
          "target": "show"
        }
      ],
      "legend": [
        {
          "color": "#9f9f61",
          "title": "Boden"
        },
        {
          "color": "#dc0000",
          "title": "Gebäude"
        },
        {
          "color": "#06ad16",
          "title": "Vegetation"
        }
      ]
    }
